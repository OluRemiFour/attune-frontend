# Adaptive Goal Agent - Environment Variables
# Copy this file to .env and fill in your values

# ======================================
# Google OAuth (Gmail API)
# ======================================
# Get these from Google Cloud Console: https://console.cloud.google.com
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/callback

# ======================================
# Comet ML (Observability)
# ======================================
# Get these from Comet.ml: https://www.comet.ml
VITE_COMET_API_KEY=your_comet_api_key_here
VITE_COMET_PROJECT_NAME=adaptive-goal-agent
VITE_COMET_WORKSPACE=your_workspace_name

# ======================================
# Backend API (if using separate backend)
# ======================================
VITE_API_BASE_URL=http://localhost:3001/api

# ======================================
# Feature Flags
# ======================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_DEMO_MODE=true
